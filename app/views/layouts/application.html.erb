    <script src="https://use.fontawesome.com/4f3c89db04.js"></script>

    <script src="https://js.pusher.com/4.1/pusher.min.js"></script>

    <script type="text/javascript">
      Pusher.logToConsole = true;
    </script>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
